// linear search 

#include<iostream>
using namespace std;

int linear(int arr[],int size)
{
    int count = 0;
    for (int i = 0; i < size; i++)
    {
        if (arr[i] == 0)
        {   
            count++;
        }
    }
    return count;
    
}

int main() {
    int arr[] = {0,5,0,4,0,5,0,5,5,4,0,0};
    int ans = linear(arr,12);
    cout<<ans;
    return 0;
}
